{
  "metadata": [{
    "src": [{
      "files": ["**/bin/Release/net8.0/ConsoleAppVisuals.dll"],
      "src": "../src/ConsoleAppVisuals/"
    }],
    "dest": "references",
    "properties": {
      "TargetFramework": "net8.0"
    }
  }],
  "build": {
    "content": [
      {
        "files": ["references/**.yml", "references/index.md"]
      },
      {
        "files": [
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
